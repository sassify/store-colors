{
  "files.exclude": {
    ".vscode": false,
    "node_modules": false,
    ".editorconfig": false,
    ".gitignore": false,
    ".npmrc": false,
    "CODEOWNERS": false,
    "LICENSE": false,
    "package-lock.json": false
